<% include header %>
  <div class="row">
  <div class="center">
    <div class="col s12 offset-l2">
      <div class="col s12 m6 l4">
        <div class="card login-subscribe">
          <h4>Login</h4>
          <div class="row-login">
              <div class="row">
                <div class="input-field col s10">
                  <input id="user" type="text" class="validate">
                  <label class="active" for="user">E-mail</label>
                </div>
              </div>
              <div class="row">
                <div class="input-field col s10">
                  <input id="passwordUser" type="password" class="validate">
                  <label for="passwordUser">Senha</label>
                </div>
              </div>
          </div>
          <div class="center">
            <a id="btn-login" class="waves-effect waves-light btn">Entrar</a>
          </div> 
        </div>
      </div>
      <div class="col s12 m6 l4">
        <div class="card login-subscribe">
          <h4>Inscreva-se</h4>
            <div class="row-login">
              <div class="row">
                <div class="input-field col s10">
                  <input id="userName" type="text" class="validate">
                  <label for="userName">Nome</label>
                </div>
              </div>
              <div class="row">
                <div class="input-field col s10">
                  <input id="userEmail" type="text" class="validate">
                  <label for="userEmail">E-mail</label>
                </div>
              </div>
              <div class="row">
                <div class="input-field col s10">
                  <input id="newPassword" type="password" class="validate">
                  <label for="newPassword">Senha</label>
                </div>
              </div>
              <div class="row">
                <div class="input-field col s10">
                  <input id="passwordConfirm" type="password" class="validate">
                  <label for="passwordConfirm">Confirmar senha</label>
                </div>
              </div>
            </div>
            <div class="center">
              <a id="btn-login" class="waves-effect waves-light btn">Inscrever-se</a>
            </div> 
          </div>
      </div>
    </div>
  </div>
</div>

<% include footer%>

</script>
